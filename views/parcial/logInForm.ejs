<div style="max-width: 500px; 
            margin: 30px auto;
            padding: 10px;
            border-radius: 15px;
            color:#ffc107;
            background-color: #757170;">
            <div>
              <h3 style="background-color: #212529;
                        text-align: center;
                        border-radius: 15px;" >Ingreso de usuario</h3>
            </div>
  <form>
      <div class="mb-3">
          <label class="form-label">User Name</label>
          <input class="form-control" id="usuario">
      </div>
      <div class="mb-3">
          <label class="form-label">Password</label>
          <input type="password" class="form-control" id="contrasenia">
      </div>
      <div style="text-align:center;">
        <button 
        style="margin-bottom: 10px" 
        type="submit" 
        value="login" 
        class="btn btn-warning text-align: center">
        Log in
        </button>
      </div>
      <form action="/signUp">
        <div style="text-align:center;">
          <button 
          style="margin-bottom: 10px" 
          type="submit"
          value="SignIn" 
          class="btn btn-success text-align: center">
          Sign in
          </button>
        </div>
      </form>
  </form>
</div>